{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "demo/**/*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/video-blog.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}